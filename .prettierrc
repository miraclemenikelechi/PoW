{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "useTabs": false,
    "tabWidth": 4,
    "tailwindFunctions": ["clsx", "tw"],
    "tailwindPreserveWhitespace": true
}
